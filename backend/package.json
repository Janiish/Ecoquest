{
  "name": "ecoquest-backend",
  "version": "1.0.0",
  "description": "EcoQuest backend API with TypeScript, Express, MongoDB, Redis, and Socket.io",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:mock": "tsx watch src/server.mock.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.0",
    "redis": "^4.6.13",
    "socket.io": "^4.7.2",
    "firebase-admin": "^12.0.0",
    "cloudinary": "^1.41.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/cors": "^2.8.17",
    "@types/multer": "^1.4.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0"
  }
}
